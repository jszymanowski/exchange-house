{
  "name": "functions",
  "main": "lib/index.js",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write .",
